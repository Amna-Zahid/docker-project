pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'Hello my first build'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deployed Successfully'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing 1 2 3'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
